<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Erica Garcia's Blog" %></title>
    <link href="https://fonts.googleapis.com/css?family=Abril+Fatface|Libre+Franklin|Open+Sans" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Dancing+Script|Quicksand" rel="stylesheet">
    <link rel="shortcut icon" href="images/portfolio/favicon.png" type="image/x-icon">
    <script src="//use.typekit.net/fel5pcq.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
    <%= stylesheet_link_tag "normalize", "all" %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <%= partial "partials/analytics" %>
    <%= javascript_include_tag  "all" %>
  </head>
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
